{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "SampleMessageSchema",
    "type": "object",
    "required": ["id", "name", "lookerIds", "createdAt", "createdBy", "owner"],
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "lookerIds": {
        "type": "array",
        "additionalItems": false,
        "items": {
            "anyOf": [
                { "type": "number" },
                { "type": "string" },
                { "type": "null" }
            ]
        }
      },
      "createdAt": {
        "type": "string"
      },
      "createdBy": {
        "type": "string"
      },
      "owner": {
        "type": "string"
      }
    }
  }
  